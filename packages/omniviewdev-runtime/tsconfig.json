{
  "compilerOptions": {
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "jsx": "react",
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true,
    "rootDir": "."
  },
  "include": [
    "lib"
  ]
}
