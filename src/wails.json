{
  "$schema": "https://wails.io/schemas/config.v2.json",
  "name": "Omniview",
  "outputfilename": "Omniview",
  "frontend:install": "npm install",
  "frontend:build": "npm run build",
  "frontend:dev:watcher": "npm run dev",
  "frontend:dev:serverUrl": "auto",
  "author": {
    "name": "Joshua Pare",
    "email": "jpare@mosaiclearning.com"
  },
  "bindings": {
    "ts_generation": {
      "prefix": "",
      "suffix": "",
      "outputType": "classes"
    }
  },
  "fileAssociations": [
    {
      "ext": "icns",
      "name": "favicon",
      "description": "AppIcon",
      "iconName": "fileIcon",
      "role": "Editor"
    }
  ]
}
